{
  "name": "enmap-wrapper",
  "version": "1.0.0",
  "description": "Object Oriented Wrapper for Enmap",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:clean": "rm -rf dist && pnpm build",
    "prepublishOnly": "pnpm run build:clean",
    "dev": "tsup --watch",
    "lint": "pnpm exec eslint .",
    "lint:fix": "pnpm exec eslint . --fix",
    "format": "pnpm exec prettier --ignore-path .gitignore --write \"**/*.+(ts|json)\"",
    "update": "pnpm exec npm-check -u",
    "depcheck": "pnpm exec npm-check"
  },
  "keywords": [
    "enmap",
    "typescript",
    "wrapper"
  ],
  "author": "Matt Strenk",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.1",
    "npm-check": "^6.0.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "enmap": "^5.9.10",
    "stumper": "^2.0.0"
  }
}